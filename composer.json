{
    "name": "cborgas/php-refactorings",
    "description": "A collection of refactoring examples in PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Borgas",
            "email": "carlborgas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/log": "^3.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "DependencyBreakingTechniques"
        ],
        "exclude-from-classmap": [
            "*/*/before.php",
            "*/*/bad.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test:filter": "phpunit --filter"
    }
}
